    "react-disqus-comments": "^1.0.3",
    "react-md": "^1.1.0-alpha-5"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gh-pages": "^1.0.0",
    "stylefmt": "^6.0.0",
